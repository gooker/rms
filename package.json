{
  "name": "rms-gui-assembly",
  "version": "3.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js --open",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "analyzer": "cross-env ANALYZE=1 NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint:fix": "eslint --fix --cache --ext .js --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@umijs/fabric": "2.10.0",
    "antd": "4.18.3",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-named-asset-import": "^0.3.7",
    "bizcharts": "3.5.9",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "check-prettier": "^1.0.3",
    "classnames": "^2.2.6",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "9.1.0",
    "css-loader": "4.3.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "echarts": "4.2.1",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-saver": "^2.0.5",
    "form-render": "^1.5.8",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^5.2.0",
    "immutability-helper": "^3.1.1",
    "json2csv": "^5.0.6",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "0.11.3",
    "moment": "^2.29.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone": "^0.5.33",
    "net": "^1.0.2",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pixi-viewport": "^4.31.0",
    "pixi.js": "^6.1.3",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-ace": "^9.4.3",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-intl-universal": "^2.4.3",
    "react-loadable": "^5.5.0",
